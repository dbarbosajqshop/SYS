{
  "name": "jqstock-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "npx mocha src/components/*/tests/*/*.test.js --experimental-json-modules",
    "test:dev": "npx mocha -w src/components/*/tests/*/*.test.js --experimental-json-modules",
    "test:cov": "npx nyc npx mocha src/components/*/tests/*/*.test.js --experimental-json-modules",
    "seed:dev": "cross-env NODE_ENV=development node run-seeds.js",
    "seed:local": "cross-env NODE_ENV=localhost node run-seeds.js",
    "unseed:dev": "cross-env NODE_ENV=development node run-unseeds.js",
    "unseed:local": "cross-env NODE_ENV=localhost node run-unseeds.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "local": "cross-env NODE_ENV=localhost nodemon server.js",
    "start": "node server.js",
    "deploy:jq": "gcloud auth activate-service-account --key-file=$JQ_GCP_KEY_PATH && gcloud app deploy app-jq.yaml --project=$JQ_GCP_PROJECT_ID",
    "deploy:keni": "gcloud auth activate-service-account --key-file=$KENI_GCP_KEY_PATH && gcloud app deploy app-keni.yaml --project=$KENI_GCP_PROJECT_ID",
    "deploy:all": "npm run deploy:jq && npm run deploy:keni"
  },
  "keywords": [],
  "author": "",
  "engines": {
    "node": "20"
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/secret-manager": "^6.0.1",
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.5.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "globby": "^14.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.15.2",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.4",
    "nyc": "^17.1.0",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  }
}
